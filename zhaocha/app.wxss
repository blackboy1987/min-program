.container {
    position: relative;
    width: 100vw;
    height: 100vh;
    overflow: hidden;
    box-sizing: border-box;
    padding-top: 100rpx;
}

.flexBox {
    display: flex;
}

.flexBoxCenterAlign {
    align-items: center;
}

.bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,0.6);
    z-index: 100;
}

.toastBg {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    width: 520rpx;
    height: 700rpx;
    text-align: center;
    overflow: hidden;
}

button {
    display: inline-block;
    padding: 0!important;
    font-size: unset!important;
    background: transparent!important;
    overflow: unset!important;
}

button::after {
    border: none!important;
}

.icon_back {
    width: 20rpx;
    height: 33rpx;
    position: fixed;
    left: 0;
    padding: 30rpx;
    z-index: 10;
}

.bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,0.6);
    z-index: 100;
}

.toastBg {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    width: 600rpx;
    height: 700rpx;
    text-align: center;
    overflow: hidden;
}

.bg1 {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: -1;
    background: url(data:image/png;base64,) repeat;
}

.startGame {
    position: relative;
    width: 320rpx;
    height: 134rpx;
    margin: 20rpx auto 0;
    text-align: center;
    -webkit-text-stroke: 0.2px #ac8f63;
    background-color: #fed150!important;
    border: 1px solid #814d28!important;
    border-radius: 20rpx;
    box-shadow: 0 -6px 0px 0px #cb6f22 inset;
}

.startGame button {
    line-height: 1;
}

.startGame text {
    font-size: 50rpx;
    font-weight: bold;
    line-height: 1.6;
    color: #fff;
    -webkit-text-stroke-width: 1.2px;
}

.startGame .barrier {
    width: 200rpx;
    height: 32rpx;
    font-size: 26rpx;
    background-color: #af8338;
    color: #fff;
    border-radius: 16rpx;
    line-height: 32rpx;
}

.startGame image {
    width: 30rpx;
    height: 40rpx;
    margin: 0 4rpx 0 6rpx;
    vertical-align: middle;
}

.elec {
    -webkit-text-stroke-width: 0.8px;
    color: #fff;
    font-weight: bold;
}

.pkGameMain {
    background-color: #4cd8f9!important;
    border-color: #3366b5!important;
    box-shadow: 0 -6px 0px 0px #2375b1 inset;
}

.pkGameMain,.pkGameMain .elec {
    -webkit-text-stroke-color: #297984;
}

.pkGameMain .barrier {
    background-color: #3296b0;
}

.mainImg {
    position: relative;
    width: 297rpx;
    height: 252rpx;
    display: inline-block;
    animation: jump 3s linear infinite alternate;
}

@-webkit-keyframes jump {
    0% {
        transform: translateY(0);
    }

    33% {
        transform: translateY(-20rpx);
    }

    66% {
        transform: translateY(0rpx);
    }

    100% {
        transform: translateY(20rpx);
    }
}

@keyframes jump {
    0% {
        transform: translateY(0);
    }

    33% {
        transform: translateY(-20rpx);
    }

    66% {
        transform: translateY(0rpx);
    }

    100% {
        transform: translateY(20rpx);
    }
}

@-webkit-keyframes moreGameIcon {
    0% {
        transform: rotate(0);
    }

    10% {
        transform: rotate(10deg);
    }

    20% {
        transform: rotate(-10deg);
    }

    30% {
        transform: rotate(10deg);
    }

    40% {
        transform: rotate(-10deg);
    }

    50% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(0);
    }
}

@keyframes moreGameIcon {
    0% {
        transform: rotate(0);
    }

    10% {
        transform: rotate(10deg);
    }

    20% {
        transform: rotate(-10deg);
    }

    30% {
        transform: rotate(10deg);
    }

    40% {
        transform: rotate(-10deg);
    }

    50% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(0);
    }
}

.leftImage,.rightImage {
    margin-top: 50rpx;
}

.leftMoreGame image,.rightImage image {
    border-radius: 50%;
    width: 100rpx;
    height: 100rpx;
    animation: moreGameIcon 3s infinite;
}

.toastImg {
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    width: 100%;
    height: 570rpx;
    border-radius: 20rpx 20rpx 0 0;
    overflow: hidden;
}

.engTitle {
    position: relative;
    line-height: 90rpx;
    padding-top: 10rpx;
    text-align: center;
    background-color: #5f7ff6;
    color: #fff;
