@-webkit-keyframes animation {
    0% {
        transform: translate(0,0);
    }

    50% {
        transform: translate(0,-5px);
    }

    100% {
        transform: translate(0,0);
    }
}

@keyframes animation {
    0% {
        transform: translate(0,0);
    }

    50% {
        transform: translate(0,-5px);
    }

    100% {
        transform: translate(0,0);
    }
}

@-webkit-keyframes weiyione {
    0% {
        transform: translate(0,0) scale(1);
        opacity: 1;
    }

    50% {
        transform: translate(50px,25px) scale(.7);
        opacity: .5;
    }

    100% {
        transform: translate(100px,50px) scale(.4);
        opacity: 0;
    }
}

@keyframes weiyione {
    0% {
        transform: translate(0,0) scale(1);
        opacity: 1;
    }

    50% {
        transform: translate(50px,25px) scale(.7);
        opacity: .5;
    }

    100% {
        transform: translate(100px,50px) scale(.4);
        opacity: 0;
    }
}

@-webkit-keyframes weiyitwo {
    0% {
        transform: translate(0,0) scale(1);
        opacity: 1;
    }

    50% {
        transform: translate(50px,-10px) scale(.7);
        opacity: .5;
    }

    100% {
        transform: translate(100px,-20px) scale(.4);
        opacity: 0;
    }
}

@keyframes weiyitwo {
    0% {
        transform: translate(0,0) scale(1);
        opacity: 1;
    }

    50% {
        transform: translate(50px,-10px) scale(.7);
        opacity: .5;
    }

    100% {
        transform: translate(100px,-20px) scale(.4);
        opacity: 0;
    }
}

@-webkit-keyframes weiyithree {
    0% {
        transform: translate(0,0) scale(1);
        opacity: 1;
    }

    50% {
        transform: translate(-50px,10px) scale(.7);
        opacity: .5;
    }

    100% {
