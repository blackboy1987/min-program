    background: #dcdcdc;
}

.btn {
    width: 550rpx;
    height: 80rpx;
    border-radius: 40px;
    font-size: 30rpx;
    line-height: 80rpx;
    text-align: center;
}

.btn::after {
    display: none;
}

.input-all {
    position: absolute;
    right: 0rpx;
    bottom: 7rpx;
    width: 80rpx;
    height: 40rpx;
    border: 1px solid #5271ff;
    border-radius: 20px;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    text-align: center;
    line-height: 40rpx;
    color: #5271ff;
}

.input-withdraw {
    margin-top: 46rpx;
    border: 1px solid #5271ff;
    font-family: PingFangSC-Medium;
    color: #5271ff;
    background: #fff;
}

.input-invite {
    margin-top: 36rpx;
    background-image: linear-gradient(-90deg,#5271ff 2%,#5d8bf7 97%);
    color: #fff;
}

.ad {
    margin: 50rpx auto 0;
    width: 690rpx;
    height: 195rpx;
    background: #ebebeb;
}

.tip {
    width: 678rpx;
    margin: 0 auto;
}

.tip-tt {
    margin-top: 50rpx;
    font-family: PingFangSC-Medium;
    font-size: 26rpx;
    color: #666;
}

.tip-content {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: grey;
}

.pop-bg {
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    z-index: 999;
}

.pop-box {
    position: relative;
    width: 648rpx;
    height: 474rpx;
    background: #fff;
    border-radius: 12rpx;
}

.pop-tt {
    margin-top: 48rpx;
    font-family: PingFangSC-Semibold;
    width: 100%;
    font-size: 42rpx;
    text-align: center;
    color: #333;
}

.pop-content {
    margin-top: 44rpx;
    font-family: PingFangSC-Regular;
    color: #666;
}

.pop-btn,.pop-content {
    font-size: 36rpx;
    text-align: center;
}

.pop-btn {
    margin: 62rpx auto 0;
    width: 420rpx;
    height: 90rpx;
    background-image: linear-gradient(-90deg,#26bcc5 2%,#1dd49e);
    box-shadow: 0 2px 2px 0 rgba(38,188,197,.4);
    border-radius: 50px;
    font-family: PingFangSC-Medium;
    line-height: 44px;
    color: #fff;
}

.pop-btn::after {
    display: none;
}

.pop-close {
    position: absolute;
    top: 30rpx;
    right: 30rpx;
    width: 29rpx;
    height: 30rpx;
    background-size: 100% 100%;
}

.mengban {
    background: rgba(0,0,0,.5);
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 1000;
    overflow: hidden;
}

.dialog1 {
    top: 39%;
}

.dialog,.dialog1 {
    background: #fff;
    position: absolute;
    z-index: 1001;
    transform: translate(-50%,-50%);
    left: 50%;
    border-radius: 12rpx;
}

.dialog {
    top: 50%;
}

.nobeijing {
    background: none;
}

.closebtn {
    position: absolute;
