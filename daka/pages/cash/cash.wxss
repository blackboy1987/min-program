}

.withdraw-num {
    font-size: 76rpx;
    color: #fff;
    text-align: center;
    font-family: DIN Alternate;
}

.input-box {
    margin: 32rpx auto 0;
    box-sizing: border-box;
    padding: 37rpx 54rpx;
    width: 690rpx;
    height: 496rpx;
    border-radius: 12rpx;
    background: #fff;
    box-shadow: 0 2rpx 5rpx 0 rgba(0,0,0,.1);
    overflow: hidden;
}

.input-box-height {
    height: 600rpx;
}

.input-box-cash-height {
    height: 800rpx;
}

.input-model-box {
    position: relative;
    margin-top: 30rpx;
    display: flex;
    justify-content: flex-start;
    align-items: flex-end;
    font-size: 48rpx;
    font-family: DIN Alternate;
    color: #333;
}

.input-model {
    margin-left: 12rpx;
    height: 60rpx;
    width: 450rpx;
}

.input-placeholder {
    color: #dcdcdc;
