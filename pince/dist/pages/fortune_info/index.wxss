.bg1 .b_cloud {
  position: relative;
}

.bg1 .box {
  margin-top: 100rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.title {
  position: absolute;
  top: -5rpx;
  right: 0;
  text-align: right;
  font-size: 30rpx;
  margin: 50rpx 38rpx 0 0;
  display: block;
  color: #fff;
  width: 152rpx;
  line-height: 30rpx;
  padding: 10rpx 0;
  z-index: 99;
}

.title .ico_change {
  background: url(https://api.xzw.com/static/image/xcx/ico_change.png) no-repeat 0 0;
  width: 30rpx;
  height: 30rpx;
  background-size: 30rpx;
  float: left;
}

.box .box_t {
  position: relative;
  margin-top: -110rpx;
  height: 305rpx;
}

.box .box_t .dt {
  width: 236rpx;
  position: absolute;
  bottom: 0;
  left: -10rpx;
}

.box .box_t .dt .dt_img {
  width: 236rpx;
  height: 236rpx;
}

.box .box_t .dt .dt_title {
  font-size: 38rpx;
  color: #5590e3;
  text-align: center;
  display: block;
  font-weight: bold;
}

.box .box_t .dd {
  width: 360rpx;
  position: absolute;
  bottom: 0;
  right: 10rpx;
}

.box .box_t .dd .star1 {
  margin: 10rpx 0;
}

.box .box_t .dd .dd_title {
  font-size: 22rpx;
  color: #999;
  display: block;
  padding-left: 5rpx;
  font-weight: bold;
}

.box .box_t .dd .dd_text {
  font-size: 33rpx;
  display: block;
  padding-left: 5rpx;
  line-height: 50rpx;
  font-weight: bold;
  color: #555;
}

.box .ul {
  font-size: 28rpx;
  margin-top: 30rpx;
}

.box .ul .li {
  display: inline-block;
  width: 50%;
  line-height: 48rpx;
  height: 48rpx;
  overflow: hidden;
}

.box .ul .li .star2 {
  float: left;
  margin-top: 10rpx;
}

.box .ul .li_title {
  color: #999;
  margin-right: 15rpx;
  float: left;
}

.box .ul .li_text {
  color: #555;
  float: left;
  display: block;
  width: 190rpx;
  height: 48rpx;
  overflow: hidden;
}

.box .content {
  font-size: 30rpx;
  line-height: 56rpx;
  border-top: 1rpx solid #e6ebff;
  margin-top: 30rpx;
  padding-top: 30rpx;
  color: #555;
}

.box .content .c_title {
  color: #5590e3;
  display: block;
  font-weight: bold;
}

.box .content .c_info {
  display: block;
  margin-bottom: 20rpx;
}

.tab_date {
  position: fixed;
  bottom: 0;
  background-size: 750rpx 120rpx;
  width: 750rpx;
  height: 120rpx;
  font-size: 36rpx;
  z-index: 5;
  text-align: center;
}

.tab_date__img {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}

.tab_date .tabli {
  position: relative;
  width: 120rpx;
  color: #d1d2ff;
  display: inline-block;
  text-align: center;
  line-height: 56rpx;
  margin-top: 46rpx;
}

.tab_date .tabli_on {
  background: #fff;
  height: 56rpx;
  border-radius: 56rpx;
  color: #01002c;
}

.mini {
  position: relative;
  bottom: -100rpx;
  padding-bottom: 122rpx;
}

.mini .li text {
  color: rgba(255,255,255,0.75);
}

.sharebtn {
  background: #d2e1ff;
  font-size: 38rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #5b86e9;
  width: 100%;
  left: 0;
  border-radius: 0 0 14rpx 14rpx;
  margin-top: 50rpx;
  margin-bottom: 20rpx;
}

.sharebtn image {
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
}

.ys {
  display: block;
  margin: 0 auto;
  width: 200rpx;
  height: 100rpx;
}

.round-click {
  height: 120rpx;
  width: 120rpx;
  background-color: #d92a2a;
  border-radius: 100%;
  position: fixed;
  bottom: 300rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9999;
}

.round-click navigator {
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}

.inner {
  margin-bottom: 100rpx;
}

.ad-wrap {
  margin-bottom: 140rpx;
}

